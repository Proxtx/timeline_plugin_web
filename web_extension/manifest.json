{
  "manifest_version": 2,
  "name": "TimelineWeb",
  "version": "1.0",

  "description": "Shares you page visits with timeline",

  "icons": {
    "48": "icons/icon_48.png"
  },

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["report.js"]
    }
  ],

  "permissions": [
    "http://localhost/*"
  ],
  "background": {
    "scripts": ["background.js"]
  }
}